{
  "name": "Item",
  "base": "PersistedModel",
  "strict": false,
  "idInjection": false,
  "options": {
    "validateUpsert": true
  },
  "mixins": {
    "TimeStamp": {
      "required": false
    }
  },
  "properties": {
    "description": {
      "type": "string",
      "comments": "\"GLENMORANGIE PRIDE 35 YO 1000ml\t\""
    },
    "volume": {
      "type": "number",
      "comments": "Volume in ml"
    },
    "price": {
      "type": "number",
      "comments": "Price in US Dollars"
    },
    "sku": {
      "type": "string",
      "id": true,
      "comments": "SKU (used as ID)"
    }
  },
  "validations": [],
  "relations": {
    "stores": {
      "type": "hasMany",
      "model": "Store",
      "foreignKey": "itemSku",
      "through": "Inventory"
    }
  },
  "acls": [],
  "methods": {}
}
