{
  "name": "Store",
  "base": "PersistedModel",
  "strict": false,
  "idInjection": false,
  "options": {
    "validateUpsert": true
  },
  "mixins": {
    "TimeStamp": {
      "required": false
    }
  },
  "properties": {
    "number": {
      "type": "number",
      "id": true
    },
    "name": {
      "type": "string"
    },
    "address": {
      "type": "string"
    },
    "city": {
      "type": "string"
    },
    "phone": {
      "type": "string"
    },
    "location": {
      "type": "geopoint"
    }
  },
  "validations": [],
  "relations": {
    "items": {
      "type": "hasMany",
      "model": "Item",
      "foreignKey": "storeNumber",
      "through": "Inventory"
    }
  },
  "acls": [],
  "methods": {}
}
