{
  "name": "Inventory",
  "base": "PersistedModel",
  "strict": false,
  "idInjection": false,
  "options": {
    "validateUpsert": true
  },
  "mixins": {
    "TimeStamp": {
      "required": false
    }
  },
  "properties": {
    "quantity": {
      "type": "number"
    },
    "itemSku": {
      "type": "string",
      "comments": "Item.sku"
    },
    "storeNumber": {
      "type": "number",
      "comments": "Store.number"
    }
  },
  "validations": [],
  "relations": {
    "item": {
      "type": "belongsTo",
      "model": "Item",
      "foreignKey": "itemSku"
    },
    "store": {
      "type": "belongsTo",
      "model": "Store",
      "foreignKey": "storeNumber"
    }
  },
  "acls": [],
  "methods": {}
}
